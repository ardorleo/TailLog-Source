  "scripts": {
    "start": "craco start",
    "et": "electron .",
    "build": "craco build",
    "eslint-fix": "eslint --fix ./src/**/*.js",
    "test": "craco test",
    "eject": "react-scripts eject",
    "mac-full": "npm run build && npm run mac-pack && npm run dmg",
    "mac-dmg": "electron-installer-dmg ./app/TailLog-darwin-x64/TailLog.app ./app/TailLog --overwrite",
    "pack": "electron-packager ./ --electron-version=2.0.2 --arch=x64 --out=./app --asar --overwrite --icon=./public/logo.ico --ignore=\"public|.cache|.idea|/src/|/images/|.map|yarn.lock\"",
    "mac-pack": "npm run pack --platform=darwin",
    "win-pack": "npm run pack --platform=win32"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
